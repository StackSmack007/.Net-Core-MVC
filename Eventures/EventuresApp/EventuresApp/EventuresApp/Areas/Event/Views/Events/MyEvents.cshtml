@model EventuresApp.Areas.Event.Models.myEventDto[]
@{
    ViewData["Title"] = "MyEvents";
}
<center>
    <div>
        <h2>My Events</h2>
        <hr class="ev-bg hr-th2px" />
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Start</th>
                    <th scope="col">End</th>
                    <th scope="col">Tickets</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.Length; i++)
                {
                    <tr>
                        <th scope="row">@(i + 1)</th>
                        <td>@Model[i].Name</td>
                        <td>@Model[i].Start.ToString("dd MMM yyyy hh:mm:ss", CultureInfo.InvariantCulture)</td>
                        <td>@Model[i].End.ToString("dd MMM yyyy hh:mm:ss", CultureInfo.InvariantCulture)</td>
                        <td>
                            @Model[i].TotalTicketCount
                        </td>
                    </tr>
                }
            </tbody>
        </table>
       
    </div>
</center>